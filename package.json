{
  "name": "cu-patcher-ui",
  "version": "0.1.0",
  "description": "Camelot Unchained Patcher UI",
  "license": "MPL-2.0",
  "main": "src/ts/main.tsx",
  "author": "City State Entertainment <support@citystateentertainment.com>",
  "contributors": [
    "JB (codecorsair) <jb@codecorsair.com>",
    "Mehuge <mehuge@sorcerer.co.uk>",
    "Stacey Schlenker (Sschlenker) <cloudsift@gmail.com>"
  ],
  "private": true,
  "keywords": [
    "Camelot",
    "Unchained",
    "cu",
    "cu-patcher-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/csegames/cu-patcher-ui.git"
  },
  "config": {
    "publishDirectory": "publish",
    "name": "cu-patcher-ui"
  },
  "scripts": {
    "typings": "typings install",
    "clean:tmp": "(rmdir /s /q tmp) ^& exit 0",
    "clean:tmpp": "(rmdir /s /q tmpp) ^& exit 0",
    "clean:publish": "(rmdir /s /q %npm_package_config_publishDirectory%) ^& exit 0",
    "clean": "npm run clean:tmp && npm run clean:tmpp && npm run clean:publish",
    "copy:misc": "(robocopy src\\ %npm_package_config_publishDirectory%\\ /s /xf *.js /xf *.ts /xf *.tsx /xf *.scss) ^& IF %ERRORLEVEL% LEQ 1 exit 0",
    "copy:third-party": "(robocopy src\\third-party %npm_package_config_publishDirectory%\\third-party /s) ^& IF %ERRORLEVEL% LEQ 1 exit 0",
    "copy": "npm run copy:third-party && npm run copy:misc",
    "babel": "babel tmp -d tmpp",
    "browserify": "browserify tmpp/main.js -o %npm_package_config_publishDirectory%/%npm_package_config_name%.js",
    "build:sass": "node-sass src/sass -o %npm_package_config_publishDirectory%/css",
    "build": "tsc && npm run build:sass && npm run copy -s && npm run babel && npm run browserify && npm run clean:tmp -s && npm run clean:tmpp -s",
    "prebuild": "npm run clean -s && npm run typings",
    "publish": "npm run build",
    "serve": "http-server -p 9000 %npm_package_config_publishDirectory%/"
  },
  "dependencies": {
    "camelot-unchained": "latest",
    "es6-promise": "^3.0.2",
    "eventemitter3": "^1.1.1",
    "isomorphic-fetch": "^2.2.1",
    "node-xmpp-client": "^2.1.0",
    "normalizr": "^2.0.0",
    "react": "^0.14.6",
    "react-addons-css-transition-group": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3",
    "reflux": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^13.0.0",
    "http-server": "^0.9.0",
    "node-sass": "^3.4.2",
    "typescript": "^1.8.0",
    "typings": "^0.6.8"
  }
}
